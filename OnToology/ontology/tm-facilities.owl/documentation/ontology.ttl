@prefix : <https://w3id.org/transmodel/facilities#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/transmodel/facilities#> .

<https://w3id.org/transmodel/facilities#> rdf:type owl:Ontology ;
                                           owl:versionIRI <https://w3id.org/transmodel/facilities/1.0> ;
                                           rdfs:comment "Module of the Transmodel Ontology focused on facilities. This ontology provides support to the NeTEx model. Visit https://github.com/oeg-upm/transmodel-ontology"@en ;
                                           rdfs:label "TransmodelOntologyFacilities"@en ;
                                           rdfs:seeAlso "https://github.com/oeg-upm/transmodel-ontology"@en ;
                                           :author _:genid1 ;
                                           :description "Module of the Transmodel Ontology focused on facilities. This ontology provides support to the NetEx model. Visit https://github.com/oeg-upm/transmodel-ontology"@en .

_:genid1 :mbox "adolfo.anton.bravo@upm.es"@en ;
          :name "Adolfo Anton-Bravo"@en .

#################################################################
#    Annotation properties
#################################################################

###  https://w3id.org/transmodel/facilities#author
:author rdf:type owl:AnnotationProperty .


###  https://w3id.org/transmodel/facilities#creator
:creator rdf:type owl:AnnotationProperty .


###  https://w3id.org/transmodel/facilities#description
:description rdf:type owl:AnnotationProperty ;
             rdfs:range rdfs:Literal ;
             rdfs:domain :MeetingPointService .


###  https://w3id.org/transmodel/facilities#mbox
:mbox rdf:type owl:AnnotationProperty .


###  https://w3id.org/transmodel/facilities#name
:name rdf:type owl:AnnotationProperty ;
      rdfs:range rdfs:Literal .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/transmodel/facilities#canBeLocatedIn
:canBeLocatedIn rdf:type owl:ObjectProperty ;
                rdfs:domain :Facility ;
                rdfs:range :Site ;
                rdfs:label "canBeLocatedIn"@en .


###  https://w3id.org/transmodel/facilities#containsEquipment
:containsEquipment rdf:type owl:ObjectProperty ;
                   rdfs:domain :EquipmentPlace ;
                   rdfs:range :Facility .


###  https://w3id.org/transmodel/facilities#facilityRequirement
:facilityRequirement rdf:type owl:ObjectProperty ;
                     rdfs:domain :Vehicle ;
                     rdfs:range :Facility ;
                     rdfs:comment "A classification of public transport Vehicles according to the facilities available on the Vehicle"@en ;
                     rdfs:label "facilityRequirement"@en .


###  https://w3id.org/transmodel/facilities#facilitySet
:facilitySet rdf:type owl:ObjectProperty ;
             rdfs:range :Facility ;
             rdfs:comment "Set of Facilities available for a ServiceJourney or a JourneyPart. The set may be available only for a specific VehicleType within the service (e.g. carriage equipped with low floor)"@en ;
             rdfs:label "facilitySet"@en .


###  https://w3id.org/transmodel/facilities#isUsedBy
:isUsedBy rdf:type owl:ObjectProperty ;
          rdfs:domain :Facility ;
          rdfs:range :Passenger ;
          rdfs:label "isUsedBy"@en .


###  https://w3id.org/transmodel/facilities#typeOfCateringService
:typeOfCateringService rdf:type owl:ObjectProperty ;
                       rdfs:domain :CateringService ;
                       rdfs:range :TypeOfCateringService ;
                       rdfs:label "typeOfCateringService"@en .


###  https://w3id.org/transmodel/facilities#typeOfCycleEquipment
:typeOfCycleEquipment rdf:type owl:ObjectProperty ;
                      rdfs:domain :CycleOperatingEquipment ;
                      rdfs:range :TypeOfCycleEquipment ;
                      rdfs:label "typeOfCycleEquipment" .


###  https://w3id.org/transmodel/facilities#typeOfEmergencyService
:typeOfEmergencyService rdf:type owl:ObjectProperty ;
                        rdfs:domain :EmergencyService ;
                        rdfs:range :TypeOfEmergencyService ;
                        rdfs:label "typeOfEmergencyService"@en .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/transmodel/facilities#available
:available rdf:type owl:DatatypeProperty ;
           rdfs:domain :Facility ;
           rdfs:range xsd:boolean ;
           rdfs:label "facilityAvailability" .


###  https://w3id.org/transmodel/facilities#description
:description rdf:type owl:DatatypeProperty .


###  https://w3id.org/transmodel/facilities#facilityLocation
:facilityLocation rdf:type owl:DatatypeProperty ;
                  rdfs:label "facilityLocation" .


###  https://w3id.org/transmodel/facilities#facilityName
:facilityName rdf:type owl:DatatypeProperty ;
              rdfs:domain :Facility ;
              rdfs:range rdfs:Literal ;
              rdfs:comment "The name of the facility"@en ;
              rdfs:label "facilityName"@en .


###  https://w3id.org/transmodel/facilities#freeToUse
:freeToUse rdf:type owl:DatatypeProperty ;
           rdfs:domain :Service ;
           rdfs:range xsd:boolean ;
           rdfs:comment "Characteristic of a Service that is free to use it"@en ;
           rdfs:label "freeToUse"@en .


###  https://w3id.org/transmodel/facilities#label
:label rdf:type owl:DatatypeProperty ;
       rdfs:domain :MeetingPointService ;
       rdfs:range rdfs:Literal ;
       rdfs:comment "labels for MeetingPointServices"@en ;
       rdfs:label "label"@en .


###  https://w3id.org/transmodel/facilities#name
:name rdf:type owl:DatatypeProperty .


###  https://w3id.org/transmodel/facilities#siteName
:siteName rdf:type owl:DatatypeProperty ;
          rdfs:domain :Site ;
          rdfs:range rdfs:Literal ;
          rdfs:label "siteName" .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/transmodel/facilities#AccessAreaDimensions
:AccessAreaDimensions rdf:type owl:Class ;
                      rdfs:subClassOf :VehicleAccessEquipment ;
                      rdfs:comment "Specialisation of VehicleAccessEquipment dedicated to VehicleAccessAreaDimmension"@en ;
                      rdfs:label "VehicleAccessAreaDimension"@en .


###  https://w3id.org/transmodel/facilities#AssistanceForDissabledUsers
:AssistanceForDissabledUsers rdf:type owl:Class ;
                             rdfs:subClassOf :AssistanceService ;
                             rdfs:comment "Assistance Services for dissabled users of the Public Transport Network"@en ;
                             rdfs:label "AssistanceForDissabledUsers"@en .


###  https://w3id.org/transmodel/facilities#AssistanceService
:AssistanceService rdf:type owl:Class ;
                   rdfs:subClassOf :Service ;
                   rdfs:comment "Specialisation of LocalService for Assistance providing information like language, accessibility trained staff, etc."@en ;
                   rdfs:label "AssistanceService"@en .


###  https://w3id.org/transmodel/facilities#BabyChange
:BabyChange rdf:type owl:Class ;
            rdfs:subClassOf :SanitaryEquipment .


###  https://w3id.org/transmodel/facilities#CateringService
:CateringService rdf:type owl:Class ;
                 rdfs:subClassOf :Service ;
                 rdfs:comment "Specialisation of LocalService for CateringService"@en ;
                 rdfs:label "CateringService"@en .


###  https://w3id.org/transmodel/facilities#CommunicationService
:CommunicationService rdf:type owl:Class ;
                      rdfs:subClassOf :Service ;
                      rdfs:comment "Specialisation of LocalService dedicated to communication services"@en ;
                      rdfs:label "CommunicationService"@en .


###  https://w3id.org/transmodel/facilities#Component
:Component rdf:type owl:Class ;
           rdfs:comment "Component of a Site or a Place"@en ;
           rdfs:label "Component"@en .


###  https://w3id.org/transmodel/facilities#CrossingEquipment
:CrossingEquipment rdf:type owl:Class ;
                   rdfs:subClassOf :PlaceAccessEquipment ;
                   rdfs:comment """Specialisation of PlaceAccessEquipment for CrossingEquipment

- Zebra"""@en ;
                   rdfs:label "CrossingEquipment"@en .


###  https://w3id.org/transmodel/facilities#CrossingEquipmentLights
:CrossingEquipmentLights rdf:type owl:Class ;
                         rdfs:subClassOf :CrossingEquipment .


###  https://w3id.org/transmodel/facilities#CrossingEquipmentPedestrian
:CrossingEquipmentPedestrian rdf:type owl:Class ;
                             rdfs:subClassOf :CrossingEquipment .


###  https://w3id.org/transmodel/facilities#CrossingEquipmentTactileGuideStrip
:CrossingEquipmentTactileGuideStrip rdf:type owl:Class ;
                                    rdfs:subClassOf :CrossingEquipment .


###  https://w3id.org/transmodel/facilities#CrossingEquipmentZebra
:CrossingEquipmentZebra rdf:type owl:Class ;
                        rdfs:subClassOf :CrossingEquipment .


###  https://w3id.org/transmodel/facilities#CustomerService
:CustomerService rdf:type owl:Class ;
                 rdfs:subClassOf :Service ;
                 rdfs:comment "Generic specialisation of LocalService for CustomerServices (LostProperties, MeetingPoint, Complaiants, etc.)"@en ;
                 rdfs:label "CustomerService"@en .


###  https://w3id.org/transmodel/facilities#CycleOperatingEquipment
:CycleOperatingEquipment rdf:type owl:Class ;
                         rdfs:subClassOf :PlaceEquipment ;
                         rdfs:label "CycleOperatingEquipment"@en .


###  https://w3id.org/transmodel/facilities#EmergencyService
:EmergencyService rdf:type owl:Class ;
                  rdfs:subClassOf :Service ;
                  rdfs:label "EmergencyService"@en .


###  https://w3id.org/transmodel/facilities#EntraceEquipment
:EntraceEquipment rdf:type owl:Class ;
                  rdfs:subClassOf :PlaceAccessEquipment .


###  https://w3id.org/transmodel/facilities#EntranceEquipmentBarrier
:EntranceEquipmentBarrier rdf:type owl:Class ;
                          rdfs:subClassOf :EntraceEquipment .


###  https://w3id.org/transmodel/facilities#EntranceEquipmentDoor
:EntranceEquipmentDoor rdf:type owl:Class ;
                       rdfs:subClassOf :EntraceEquipment .


###  https://w3id.org/transmodel/facilities#EntranceEquipmentRevolvingDoor
:EntranceEquipmentRevolvingDoor rdf:type owl:Class ;
                                rdfs:subClassOf :EntraceEquipment .


###  https://w3id.org/transmodel/facilities#Entrances
:Entrances rdf:type owl:Class ;
           rdfs:subClassOf :PlaceAccessEquipment ;
           rdfs:comment "Specialisation of PlaceAccessEquipment dedicated to PlaceAccessEntrance"@en ;
           rdfs:label "PlaceEntrance"@en .


###  https://w3id.org/transmodel/facilities#Equipment
:Equipment rdf:type owl:Class ;
           rdfs:subClassOf :Facility ;
           rdfs:comment """Physical equipments installed either fixed (PlaceEquipment), on-board (VehicleEquipment) ot mixed (some equipments as rubish, sanitary or ticketing).

A service (LocalService) is considered as immaterial Equipment as well."""@en ;
           rdfs:label "Equipment"@en .


###  https://w3id.org/transmodel/facilities#EquipmentPlace
:EquipmentPlace rdf:type owl:Class ;
                rdfs:subClassOf :SiteComponent ;
                rdfs:comment "An EquipmentPlace is a SiteComponent containing Equipment" ;
                rdfs:label "EquipmentPlace" .


###  https://w3id.org/transmodel/facilities#Facility
:Facility rdf:type owl:Class ;
          rdfs:comment """A named amenity available to the public at a Site or on a Service.

A Facility has no further properties than a name.

An Equipment or LocalService is used to describe the further properties provided as part of a particular Facility"""@en ;
          rdfs:label "Facility"@en .


###  https://w3id.org/transmodel/facilities#Garage
:Garage rdf:type owl:Class ;
        rdfs:subClassOf :PlaceEquipment ;
        rdfs:comment """A Facility of PlaceEquipment used for parking and maintaining
	vehicles. Parking Point in a Garage are called GaragePoint."""@en ;
        rdfs:label "Garage"@en .


###  https://w3id.org/transmodel/facilities#HeadingSign
:HeadingSign rdf:type owl:Class ;
             rdfs:subClassOf :SignEquipment .


###  https://w3id.org/transmodel/facilities#InformationBooklet
:InformationBooklet rdf:type owl:Class ;
                    rdfs:subClassOf :PassengerInformationPrintedMaterial ;
                    rdfs:comment "Information Booklet is public transport piece of Equipment dedicated to inform"@en ;
                    rdfs:label "InformationBooklet"@en .


###  https://w3id.org/transmodel/facilities#InformationDesk
:InformationDesk rdf:type owl:Class ;
                 rdfs:subClassOf :PassengerInformationTerminal ;
                 rdfs:comment "A public transport information piece of Equipment"@en ;
                 rdfs:label "InformationDesk"@en .


###  https://w3id.org/transmodel/facilities#InformationLeaflet
:InformationLeaflet rdf:type owl:Class ;
