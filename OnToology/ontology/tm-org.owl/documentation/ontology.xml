        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/org#OrganizationalUnit"/>
        <rdfs:comment xml:lang="en">A part of an organisation to which a set of responsabilities upon the data and data management may be assigned</rdfs:comment>
        <rdfs:label xml:lang="en">OrganisationPart</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/transmodel/authorities#OrganisationUnit -->


    <owl:Class rdf:about="https://w3id.org/transmodel/authorities#OrganisationUnit">
        <rdfs:subClassOf rdf:resource="https://w3id.org/transmodel/authorities#OrganisationPart"/>
        <rdfs:comment xml:lang="en">An OrganisationPart to which a set of responsabilities in an organisation for planning and control is assigned</rdfs:comment>
        <rdfs:label xml:lang="en">OrganisationalUnit</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/transmodel/authorities#RetailConsortium -->


